.rank-list-wrapper {
  padding: 0 25rpx;
}

.rank-list {
  display: flex;
  flex-wrap: wrap;
  align-items: flex-end;
  justify-content: space-between;
}

.rank-item {
  width: 340rpx;
}

.rank-item-0 {
  order: 1;
  flex: 0 0 247rpx;
}

.rank-item-1 {
  flex: 0 0 206rpx;
  order: 0;
}

.rank-item-2 {
  flex: 0 0 206rpx;
  order: 2;
}

.rank-item-3 {
  order: 3;
}

.rank-item-4 {
  order: 4;
}
